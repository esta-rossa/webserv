server 
{
	listen          80;
	host localhost;
	server_name     example.com;

	client_max_body_size    10m;

	error_page 404 /404.html  
	error_page 500 /500.html
	error_page 502 /502.html

	root /src;

	location / 
	{
		root /home;
		autoindex on;
		index index.html;
		allow_methods "GET";
	}

	location /return 
	{
	return 301 _url;
	}    


	location /cgi 
	{
	}

	location /upload 
	{
		upload_enable _;
		upload_store /files;
	}
}

